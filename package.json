{
  "private": true,
  "version": "2.0.0",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "update-icon": "npx iconfont-h5",
    "changelog": "conventional-changelog -p angular -i ./CHANGELOG.md -s -r 0"
  },
  "lint-staged": {
    "*.{js,jsx,md,json}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.less": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@formily/antd": "^2.0.0-beta.78",
    "@formily/core": "^2.0.0-beta.78",
    "@formily/react": "^2.0.0-beta.78",
    "@umijs/preset-react": "1.8.17",
    "@umijs/test": "^3.5.8",
    "antd": "^4.16.7",
    "await-to-js": "^3.0.0",
    "braft-editor": "^2.3.9",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.6",
    "lodash": "^4.17.21",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-iframe-comm": "^1.2.2",
    "react-visual-design-components": "^2.0.1",
    "umi": "^3.5.10",
    "umi-request": "^1.3.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@umijs/fabric": "^2.6.2",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-plugin-import": "^1.13.3",
    "conventional-changelog-cli": "^2.1.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "react-iconfont-cli": "^2.0.1",
    "webpack-version-file-plugin": "^0.4.0"
  }
}
